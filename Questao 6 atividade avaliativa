
//questao 6 
programa {
  funcao inicio() {
    inteiro carro_sonho = 25000 
    inteiro carro_economico = 15000 
    inteiro orcamento = 20000 
    
    se (orcamento >= carro_sonho) {
      escreva("Voce pode comprar o carro dos seus sonhos!")
    } senao {
      escreva("Você precisará optar por um carro econômico.")
    }
  }
}
